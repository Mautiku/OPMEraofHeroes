   if input.KeyCode == Enum.KeyCode.Four then
game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Characters"):WaitForChild("Saitama"):WaitForChild("Skills"):WaitForChild("Base"):WaitForChild("DeathPunch"):FireServer()

      end
    end
end)
